@page
@model TmkSelersWeb.Pages.ModeloProspecto.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Prospecto[0].TelefonoPrincipal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prospecto[0].Apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prospecto[0].Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prospecto[0].Llamado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prospecto[0].Venta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prospecto[0].ObsProspecto)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Prospecto) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TelefonoPrincipal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Llamado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Venta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ObsProspecto)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.IdProspecto">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.IdProspecto">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.IdProspecto">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
